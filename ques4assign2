#include <stdio.h>

int main() {
    int marks[] = {50, 99, 78, 99, 35};
    int n = sizeof(marks) / sizeof(marks[0]);
    int count = 0;

    printf("Students who scored 99: ");
    for (int i = 0; i < n; i++) {
        if (marks[i] == 99) {
            printf("%d ", i);
            count++;
        }
    }
    printf("\nTotal students scored 99: %d\n", count);

    return 0;
}
