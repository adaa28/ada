#include <stdio.h>

int main() {
    int marks[] = {50, 65, 78, 90, 35};
    int n = sizeof(marks) / sizeof(marks[0]);

    for (int i = 0; i < n; i++) {
        if (marks[i] % 2 == 0)
            printf("Score %d is Even\n", marks[i]);
        else
            printf("Score %d is Odd\n", marks[i]);
    }

    return 0;
}
