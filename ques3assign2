#include <stdio.h>

int main() {
    int marks[] = {50, 99, 78, 99, 35};
    int n = sizeof(marks) / sizeof(marks[0]);
    int found = 0;

    for (int i = 0; i < n; i++) {
        if (marks[i] == 99) {
            printf("First 99 scored by student at index %d.\n", i);
            found = 1;
            break;
        }
    }
    if (!found)
        printf("No student scored 99.\n");

    return 0;
}
