#include <stdio.h>

int is_prime(int num) {
    if (num <= 1)
        return 0;
    for (int i = 2; i * i <= num; i++) {
        if (num % i == 0)
            return 0;
    }
    return 1;
}

int main() {
    int arr[] = {10, 15, 23, 45, 7, 11, 29};
    int n = sizeof(arr) / sizeof(arr[0]);
    int count = 0;

    for (int i = 0; i < n; i++) {
        if (is_prime(arr[i]))
            count++;
    }
    printf("Count of prime numbers: %d\n", count);

    return 0;
}
